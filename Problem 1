public class Problem 1 {

    
    public static void main(String[] args) {
        
        int sums = 0;
        
        for (int i=1; i<1000; i++){
            if (i%3==0 || i%5==0){
                int temp=i;
               sums=sums+temp; 
            }
            
            System.out.println(sums);
        }
        
        
    }
    
}
